# Example environment variables for connecting to real LLM providers
# Copy to .env and update with your real keys (DO NOT commit secrets)

# Choose provider: openai | anthropic | stub
AUTOPR_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE
OPENAI_MODEL=gpt-4o

# Anthropic
ANTHROPIC_API_KEY=OPENAI_API_KEY=YOUR_ANTHROPIC_API_KEY_HERE
ANTHROPIC_MODEL=claude-2
